{
  "name": "aquanura",
  "version": "1.0.2",
  "description": "A modern Kintone plugin development template with Vite, hot reload, and local logging",
  "private": true,
  "scripts": {
    "check": "biome check --write",
    "format": "biome format --write",
    "fix-cert": "node logistics/plugin-deploy/fix-cert.js",
    "dev": "node logistics/plugin-deploy/vite/dev.js",
    "build": "node logistics/plugin-deploy/vite/build.js",
    "build:dev": "node logistics/plugin-deploy/vite/build.js --dev",
    "upload:dev": "node logistics/plugin-deploy/upload-dev.js",
    "upload:prod": "node logistics/plugin-deploy/upload-prod.js",
    "dev:custom": "node logistics/customization-deploy/vite/build.js --watch",
    "build:custom": "node logistics/customization-deploy/vite/build.js",
    "upload:custom:dev": "node logistics/customization-deploy/upload-dev.js",
    "upload:custom:prod": "node logistics/customization-deploy/upload-prod.js"
  },
  "keywords": ["kintone", "plugin", "vite", "template", "hot-reload"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4"
  }
}
