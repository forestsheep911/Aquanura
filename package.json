{
  "name": "aquanura",
  "version": "1.0.2",
  "description": "A modern Kintone plugin development template with Vite, hot reload, and local logging",
  "private": true,
  "scripts": {
    "lint": "biome check .",
    "check": "biome check --write",
    "format": "biome format . --write",
    "install:all": "pnpm install",
    "fix-cert": "node logistics/plugin-deploy/fix-cert.js",
    "dev": "node --max-old-space-size=4096 logistics/plugin-deploy/vite/dev.js",
    "build": "pnpm run build:prod",
    "build:prod": "node logistics/plugin-deploy/vite/build.js",
    "build:dev": "node logistics/plugin-deploy/vite/build.js --dev",
    "upload:dev": "node logistics/plugin-deploy/upload-dev.js",
    "upload:prod": "node logistics/plugin-deploy/upload-prod.js",
    "deploy:dev": "pnpm run build:dev && pnpm run upload:dev",
    "deploy:prod": "pnpm run build:prod && pnpm run upload:prod",
    "deploy:watch": "pnpm run deploy:dev:watch",
    "deploy:dev:watch": "node logistics/plugin-deploy/vite/deploy-watch.js",
    "deploy:prod:watch": "node logistics/plugin-deploy/vite/deploy-watch.js --target prod"
  },
  "keywords": [
    "kintone",
    "plugin",
    "vite",
    "template",
    "hot-reload"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "ora": "^5.4.1"
  }
}
