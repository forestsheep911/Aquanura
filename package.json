{
  "name": "aquanura",
  "version": "1.0.2",
  "description": "A modern Kintone plugin development template with Vite, hot reload, and local logging",
  "private": true,
  "scripts": {
    "check": "biome check --write",
    "format": "biome format --write",
    "fix-cert": "pnpm --filter kintone-plugin-deploy run fix-cert",
    "dev": "pnpm --filter kintone-plugin-deploy run dev",
    "build": "pnpm --filter kintone-plugin-deploy run build",
    "upload:dev": "pnpm --filter kintone-plugin-deploy run upload-dev",
    "upload:prod": "pnpm --filter kintone-plugin-deploy run upload-prod"
  },
  "keywords": ["kintone", "plugin", "vite", "template", "hot-reload"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4"
  }
}
