# ===========================================
# Kintone Customization - Environment Configuration
# Copy this file to .env and fill in your actual values
# ===========================================

# === Common Kintone Credential ===
# Base URL and credentials generally shared
KINTONE_BASE_URL=https://your-domain.cybozu.com/
KINTONE_USERNAME=your-username
KINTONE_PASSWORD=your-password

# === Customization App IDs ===
# App ID for Development Environment
KINTONE_CUSTOMIZATION_APP_ID_DEV=

# App ID for Production Environment
KINTONE_CUSTOMIZATION_APP_ID_PROD=

# === Dev Server Configuration ===
VITE_LOG_LEVEL=info
