# ===========================================
# Kintone Customization - Environment Configuration
# Copy this file to .env and fill in your actual values
# ===========================================



# === Development Environment ===
KINTONE_BASE_URL_DEV=https://your-dev-domain.cybozu.com/
KINTONE_USERNAME_DEV=your-dev-username
KINTONE_PASSWORD_DEV=your-dev-password
KINTONE_CUSTOMIZATION_APP_ID_DEV=

# === Production Environment ===
KINTONE_BASE_URL_PROD=https://your-prod-domain.cybozu.com/
KINTONE_USERNAME_PROD=your-prod-username
KINTONE_PASSWORD_PROD=your-prod-password
KINTONE_CUSTOMIZATION_APP_ID_PROD=

# === Dev Server Configuration ===
VITE_LOG_LEVEL=info
